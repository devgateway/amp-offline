{
  "I18N": {
    "OPTIONS": {
      "development": {
        "lng": "en",
        "lngs": ["en", "es"],
        "fallbackLng": "en",
        "ns": [
          "translations"
        ],
        "defaultNS": "translations",
        "fallbackNS": "common",
        "backend": {
          "loadPath": "./locales/{{ns}}.{{lng}}.json"
        }
      },
      "production": {
        "lng": "en",
        "lngs": ["en", "es"],
        "fallbackLng": "en",
        "ns": [
          "translations"
        ],
        "defaultNS": "translations",
        "fallbackNS": "common",
        "backend": {
          "loadPath": "./locales/{{ns}}.{{lng}}.json"
        }
      }
    }
  }
}
