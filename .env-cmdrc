{
  "production": {
    "__": "!!! ALERT !!! Not used from packaged app. Set env var to 0 by default as a workaround.",
    "NODE_ENV": "production",
    "NODE_OPTIONS": "--max-old-space-size=8192",
    "DISABLE_UPDATE": 0,
    "USE_TEST_AMP_URL": 0,
    "DISABLE_CHANGELOGS": 0
  },
  "development": {
    "NODE_ENV": "development",
    "NODE_OPTIONS": "--max-old-space-size=8192",
    "HOT": "1",
    "SANITY_APP_DEBUG_WINDOW": 0,
    "SKIP_SANITY_CHECK": 0,
    "FORCE_LOGGER": false,
    "DISABLE_UPDATE": 1,
    "APPIMAGE": "./update",
    "USE_TEST_AMP_URL": 1,
    "CHECK_URL_CHANGES": 0,
    "PRE_SYNC": 0,
    "DISABLE_CHANGELOGS": 0
  },
  "test": {
    "NODE_ENV": "test",
    "BABEL_DISABLE_CACHE": "1",
    "DISABLE_CHANGELOGS": 0,
    "LOG_TO_CONSOLE": 0
  }
}
