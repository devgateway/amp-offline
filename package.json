{
  "name": "amp-offline",
  "productName": "AMP Offline",
  "version": "0.0.2",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test-mocha": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --timeout 30000 --retries 2 --compilers js:babel-register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test-electron-mocha": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 electron-mocha --compilers js:babel-register --recursive --renderer test/**/*.spec.electron-mocha.js",
    "test": "npm run test-mocha && npm run test-electron-mocha",
    "test-watch": "npm test -- --watch",
    "test-mocha-watch": "npm run test-mocha -- --watch",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --retries 2 --compilers js:babel-register --require ./test/setup.js ./test/e2e.js",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern libs/ --ext .jsx,.js --format=node_modules/eslint-formatter-pretty app test *.js",
    "hot-server": "cross-env NODE_ENV=development FORCE_LOGGER=false node --max_old_space_size=2096 -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.development",
    "postinstall": "concurrently \"install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "dev": "npm run hot-server -- --start-hot",
    "package": "npm run build && build --publish never",
    "package-win": "npm run build && build --win --x64 --ia32",
    "package-linux": "npm run build && build --linux",
    "package-mac": "npm run build && build -m",
    "package-all": "npm run build && build -mwl",
    "cleanup": "mop -v",
    "flow": "flow"
  },
  "build": {
    "appId": "dg.AmpOffline",
    "category": "public.app-category.tools",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/",
      "node_modules/",
      "libs/",
      "app.html",
      "main.js",
      "main.js.map",
      "package.json",
      "master-translations.en.json"
    ],
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    }
  },
  "directories": {
    "buildResources": "resources",
    "output": "release"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "homepage": "",
  "author": {
    "name": "DG",
    "email": "something@developmentgateway.org",
    "url": "http://"
  },
  "devDependencies": {
    "asar": "^0.12.3",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-tcomb": "^0.3.21",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "boiler-room-custodian": "^0.4.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "concurrently": "^3.1.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "devtron": "^1.4.0",
    "electron": "^1.4.4",
    "electron-builder": "^8.3.0",
    "electron-devtools-installer": "^2.0.1",
    "enzyme": "^2.5.1",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.7.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-flowtype-errors": "^1.5.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^6.6.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.7.1",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.47.0",
    "html-webpack-plugin": "^2.24.0",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^3.1.2",
    "react-bootstrap-table": "^3.4.1",
    "redux-logger": "^2.7.4",
    "redux-mock-store": "^1.2.1",
    "request": "^2.79.0",
    "sinon": "^1.17.6",
    "spectron": "^3.4.0",
    "string-replace-webpack-plugin": "^0.0.5",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "tcomb": "^3.2.15",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.1",
    "webpack-merge": "^0.15.0",
    "webpack-validator": "^2.2.9"
  },
  "dependencies": {
    "babel-plugin-webpack-alias": "^2.1.2",
    "bluebird": "^3.4.6",
    "bluebird-queue": "0.0.8",
    "bunyan": "^1.8.10",
    "crypto-js": "^3.1.8",
    "electron-debug": "^1.0.1",
    "electron-mocha": "^3.4.0",
    "font-awesome": "^4.7.0",
    "fs-extra": "^2.0.0",
    "i18next": "^4.1.1",
    "i18next-sync-fs-backend": "^0.1.0",
    "i18next-xhr-backend": "^1.2.1",
    "jsonschema": "^1.1.1",
    "material-ui": "^0.18.6",
    "moment": "^2.18.1",
    "nedb": "^1.8.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.5.10",
    "rc-menu": "^5.0.7",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.4.2",
    "react-bootstrap": "^0.30.6",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "react-scrollspy": "^3.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-promise-middleware": "^4.2.1",
    "redux-thunk": "^2.1.0",
    "source-map-support": "^0.4.6",
    "stack-trace": "0.0.9",
    "stringify-object": "^3.1.0",
    "text-encoding": "^0.6.4",
    "underscore": "^1.8.3"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  },
  "config": {
    "ampServerUrl": "amp-niger-future-v3-0-tc9.ampsite.net",
    "ampServerPort": "80",
    "ampServerProtocol": "http"
  }
}
